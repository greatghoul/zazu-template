{
  "name": "zazu-youdao",
  "version": "1.0.0",
  "description": "有道翻译的 Zazu 插件",
  "blocks": {
    "input": [
      {
        "id": "translate",
        "type": "PrefixScript",
        "keyword": "t",
        "space": true,
        "args": "Required",
        "script": "src/index.js",
        "debounce": 100,
        "title": "有道翻译",
        "subtitle": "Goto Zazu Template GitHub Repo",
        "icon": "fa-search",
        "connections": ["Copy"]
      },
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
